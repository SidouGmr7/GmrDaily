{
    "functions": {
        "api/server.js": {
            "memory": 1024,
            "includeFiles": "db.json"
        }
    },
    "routes": [
        {
            "src": "/api/.*",
            "dest": "/api/serve.js"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ]
}
